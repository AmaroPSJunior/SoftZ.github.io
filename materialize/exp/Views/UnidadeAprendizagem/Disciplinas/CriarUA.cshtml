                <i class="fas fa-angle-right" aria-hidden="true"></i><h2 class="sl-subtitulo sl-navy-blue">@Model.Item4</h2>
            </div>
        </div>
        @Html.Partial("~/Views/UnidadeAprendizagem/Disciplinas/Partial/wizard.cshtml")
