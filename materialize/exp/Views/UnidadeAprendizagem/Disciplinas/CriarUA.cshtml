            <div class="divPartial">
                @{ Html.RenderAction("LoadDefinicoesGerais", "UnidadeAprendizagem", new { codcurso = Model.Item1, coddisciplina = Model.Item2, codunidadeaprendizagem = Model.Item3 }); }
